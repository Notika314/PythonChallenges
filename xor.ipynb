    "def xor(s,t):\n",
    "    res = \"\"\n",
    "    for i in range(len(s)):\n",
    "        if s[i] == t[i]:\n",
    "            res = res+ '0';\n",
    "        else:\n",
    "            res = res + '1';\n",
    "\n",
    "    return res\n",
    "\n",
    "print(xor(\"0101\",\"1010\"))\n",
    "    "
